//Full Rex Extraction
//Author: Bryan J Muscedere

SCHEME TUPLE :
//Nodes
$INHERIT	cArchitecturalNds	cRoot
$INHERIT	cAsgNds			cRoot
$INHERIT	rosMsg			cRoot
$INHERIT	cClass			cAsgNds
$INHERIT	cFunction		cAsgNds
$INHERIT	cVariable		cAsgNds
$INHERIT	rosTopic		rosMsg
$INHERIT	rosPublisher		rosMsg
$INHERIT	rosSubscriber		rosMsg
$INHERIT	rosNodeHandle		rosMsg

//Relationships
contain		cRoot		cRoot
publish		rosPublisher	rosTopic
subscribe	rosTopic	rosSubscriber
reference	rosNodeHandle	rosMsg
advertise	rosPublisher	rosTopic
subscribe	rosSubscriber	rosTopic
call		cAsgNds		cAsgNds
write		cFunction	cVariable
read		cVariable	cFunction		

SCHEME ATTRIBUTE :
$ENTITY {
	x
	y
	width
	height
	label
}

cRoot {
	elision = contain
	color = (0.0 0.0 0.0)
	file
	line
	name
}

cAsgNds {
	beg
	end
	file
	line
	value
	color = (0.0 0.0 0.0)
}

cArchitecturalNds {
	class_style = 4
	color = (0.0 0.0 1.0)
	color = (0.0 0.0 0.0)
}

rosMsg {
	class_style = 4
	color = (0.0 0.0 1.0)
}

cFunction {
	filename
	isStatic
	isConst
	isVolatile
	isVariadic
	visibility
	color = (1.0 0.0 0.0)
	labelcolor = (0.0 0.0 0.0)
}

cVariable {
	filename
	scopeType
	isStatic
	isPublisher = 0
	isSubscriber = 0
}

cClass {
	filename
	baseNum
	color = (0.2 0.4 0.1)
	labelcolor = (0.0 0.0 0.0)
}

rosSubscriber {
	bufferSize
	numAttributes
	rosNumber	
	callbackFunc
}

rosPublisher {
	numAttributes
	rosNumber
}
