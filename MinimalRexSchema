	class_style = 4
	color = (0.0 0.0 1.0)
	color = (0.0 0.0 0.0)
}

rosMsg {
	class_style = 4
	color = (0.0 0.0 1.0)
}

cFile {
	class_style = 2
	color = (0.9 0.9 0.9)
	labelcolor = (0.0 0.0 0.0)
}

cFunction {
	filename
	isStatic
	isConst
	isVolatile
	isVariadic
	visibility
	color = (1.0 0.0 0.0)
	labelcolor = (0.0 0.0 0.0)
}

cVariable {
	filename
	scopeType
	isStatic
	isPublisher = 0
	isSubscriber = 0
}

cClass {
	filename
	baseNum
	color = (0.2 0.4 0.1)
	labelcolor = (0.0 0.0 0.0)
}

cEnum {
	filename
	color = (0.9 0.2 0.5)
	labelcolor = (0.0 0.0 0.0)
}

cEnumConst {
	filename
	color = (0.9 0.2 0.5)
	labelcolor = (0.0 0.0 0.0)
}

